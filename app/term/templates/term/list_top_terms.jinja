{% extends './base.jinja' %}
{% if sort_order == 'ascending' %}
{% set sort_order = 'descending'  %}
{% elif sort_order== 'descending'%}
{% set sort_order = 'ascending'  %}
{% endif %}


{% block title %}
Browse Terms - alphabetical
{% endblock %}

{% block headline %}
Browse terms - alphabetical
{% endblock headline %}

{% block content %}
{% include 'term/_subnav.jinja' %}

{% for term in term_list  %}
<div class="row py-1">
    <div class="col">
        <a href="{{ url_for('term.show_alternate_terms', term_string= term[0]) }}">{{ term[0] }}</a><br>
    </div>
</div>
{% endfor %}


{% endblock content %}