{% extends './base.jinja' %}

{% block title %}
Contribute to YAMZ
{% endblock %}

{% block headline %}
Imported terms
{% endblock headline %}

{% block content %}


{% for term in term_dict %}
<hr>
<dl class="row">
  
  
  <dt class="col-sm-2">Term</dt>
  <dd class="col-sm-10">{{ term.term }}</dd>

  
  <dt class="col-sm-2">Definition</dt>
  <dd class="col-sm-10">{{ term.definition }}</dd>

  <dt class="col-sm-2">Examples</dt>
  <dd class="col-sm-10">
    {{term.examples}}
  </dd>

  
</dl>
{% endfor %}


{% endblock content%}